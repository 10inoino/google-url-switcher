{
  "name": "G URL Switcher",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Switch URL for Google Workspace services",
  "background": {
    "service_worker": "index.js"
  },
  "icons": {
    "128": "128.png"
  },
  "permissions": ["contextMenus", "scripting"],
  "host_permissions": ["<all_urls>"]
}
